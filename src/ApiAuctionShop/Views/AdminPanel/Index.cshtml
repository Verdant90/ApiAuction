@model AdminMenuModel
@section scripts{
    <link href="~/css/bootstrap-admin-theme.css" rel="stylesheet" asp-file-version="true" />
   
   
}
@Html.Partial("_AdminLeftMenu")
<div class="col-md-10">
    <div class="row">
        <div class="bootstrap-admin-content-title">
            <h1>
                Kainos Auction System Admin Panel
            </h1>
        </div>
    </div>
    <div class="row bootstrap-admin-no-edges-padding">
        <div class="col-md-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="text-muted bootstrap-admin-box-title">Authors</div>
                </div>
                <div class="bootstrap-admin-panel-content">
                    <ul>
                        <li>Michał Recław</li>
                        <li>Patryk Sadowski</li>
                        <li>Szymon Duraj</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="text-muted bootstrap-admin-box-title">Links</div>
                </div>
                <div class="bootstrap-admin-panel-content">
                    <ul>
                        <li>
                            <a href="https://github.com/Verdant90/ApiAuction" target="_blank">Github Repository</a>
                        </li>
                        <li>
                            <a href="http://projektgrupowykainos.acunote.com/projects/52029/sprints">Acunote Sprint Log</a>
                        </li>
                        <li>
                            <a href="https://github.com/Verdant90/ApiAuction/archive/master.zip">Download (.zip package)</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="row bootstrap-admin-no-edges-padding">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div class="text-muted bootstrap-admin-box-title">Readme</div>
                </div>
                <div class="bootstrap-admin-panel-content">
                    <p>soon</p>
                </div>
            </div>
        </div>
    </div>
    <div class="row bootstrap-admin-no-edges-padding">
        
    </div>
</div>
